{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "age": {
      "type": "string",
      "pattern": "^(0?[1-9]|[1-9][0-9])$"
    },
    "color": {
      "type": "string",
      "enum": ["Green"]
    },
    "highScore": {
      "type": "integer",
      "minimum": 0
    },
    "image": {
      "type": "string",
      "format": "uri"
    },
    "name": {
      "type": "string"
    },
    "state": {
      "type": "string"
    }
  },
  "required": ["age", "color", "highScore", "image", "name", "state"]
}
